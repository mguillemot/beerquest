- invite_msg = t("invite.message#{@invite}")
:javascript
  var initialized = function() {
    FB.ui({
      method: 'apprequests',
      title: "#{t('invite.selector')}",
      message: "#{invite_msg}",
      data: "uid=#{@me.id},msg=#{@invite}"
      //filters: [{name: "", user_ids: []}]
    }, function(response) {
      if (response && response.request_ids) {
        console.log(response.request_ids.join(','));
        window.location = "/";
      } else {
        window.location = "/";
      }
    });
  };